<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <include
        android:id="@+id/titleBar"
        layout="@layout/layout_title_bar"/>
    <TextView
        android:textSize="@dimen/qc_sp_18"
        android:textStyle="bold"
        android:textColor="@color/device_scan_item_title"
        android:id="@+id/tv_warming_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/qc_dp_26"
        android:layout_marginTop="@dimen/qc_dp_10"
        android:text="@string/qc_text_469"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/titleBar"/>
    <TextView
        android:textSize="@dimen/qc_sp_14"
        android:textColor="@color/device_scan_item_title"
        android:id="@+id/tv_warming_desc"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/qc_dp_5"
        android:text="@string/qc_text_470"
        android:layout_marginStart="@dimen/qc_dp_26"
        android:layout_marginEnd="@dimen/qc_dp_10"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_warming_title"/>
</androidx.constraintlayout.widget.ConstraintLayout>
