<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:paddingLeft="@dimen/qc_dp_10"
    android:paddingRight="@dimen/qc_dp_10"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <com.qcwireless.qcwatch.ui.base.view.QSettingItemWithClickSystem
        android:id="@+id/qc_debug"
        android:visibility="gone"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:minHeight="@dimen/qc_dp_54"
        app:LtextColor="@color/debug_common_1"
        app:isShowRightText="true"
        app:layout_constraintTop_toTopOf="parent"
        app:leftText="@string/qc_text_596"
        app:rightStyle="iconSwitch"
        app:rightText=""
        app:rightTextColor="@color/debug_common_2"/>
    <com.qcwireless.qcwatch.ui.base.view.QSettingItemWithClickSystem
        android:id="@+id/qc_test"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/qc_dp_40"
        android:minHeight="@dimen/qc_dp_54"
        app:LtextColor="@color/debug_common_1"
        app:isShowRightText="true"
        app:layout_constraintTop_toBottomOf="@+id/qc_debug"
        app:leftText="@string/qc_text_595"
        app:rightStyle="iconSwitch"
        app:rightText=""
        app:rightTextColor="@color/debug_common_2"/>
    <Button
        android:id="@+id/btn_copy"
        android:visibility="gone"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="拷贝文件到download文件夹下面"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/qc_debug"/>
    <TextView
        android:id="@+id/text_3"
        android:visibility="gone"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="class bluetooth"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_copy"/>
    <TextView
        android:id="@+id/text_4"
        android:visibility="gone"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="百度文心测试"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"/>
    <TextView
        android:id="@+id/tv_show"
        android:layout_width="match_parent"
        android:layout_height="@dimen/qc_dp_200"
        app:layout_constraintBottom_toBottomOf="parent"/>
</androidx.constraintlayout.widget.ConstraintLayout>
