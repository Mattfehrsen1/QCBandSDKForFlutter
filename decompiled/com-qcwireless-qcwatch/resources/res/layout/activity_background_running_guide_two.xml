<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="@color/color_F9F9F9"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <include
        android:id="@+id/title_bar"
        layout="@layout/layout_title_bar"/>
    <androidx.core.widget.NestedScrollView
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_marginLeft="@dimen/qc_dp_10"
        android:layout_marginRight="@dimen/qc_dp_10"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/title_bar">
        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <TextView
                android:textSize="20sp"
                android:textColor="@color/app_bg_run_common_1"
                android:id="@+id/tv_notification"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="@dimen/dp_10"
                android:text="@string/qc_text_399"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintTop_toTopOf="parent"/>
            <ImageView
                android:id="@+id/image_1_right"
                android:padding="@dimen/dp_10"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@mipmap/common_right_1"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toTopOf="@+id/tv_notification"/>
            <TextView
                android:textSize="14sp"
                android:textColor="@color/color_FFFFFF"
                android:gravity="center"
                android:id="@+id/tv_status_text_1"
                android:background="@drawable/bg_corners_bg_text_2"
                android:paddingLeft="10dp"
                android:paddingRight="10dp"
                android:layout_width="wrap_content"
                android:layout_height="40dp"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="10dp"
                android:text="@string/qc_text_410"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/tv_notification"/>
            <TextView
                android:textSize="20sp"
                android:textColor="@color/app_bg_run_common_1"
                android:id="@+id/tv_battery_white"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="@dimen/dp_10"
                android:text="@string/qc_text_401"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/tv_status_text_1"/>
            <TextView
                android:textSize="14sp"
                android:textColor="@color/app_bg_run_common_1"
                android:id="@+id/tv_battery_white_sub"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="@dimen/dp_10"
                android:maxWidth="300dp"
                android:text="@string/qc_text_409"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/tv_battery_white"/>
            <ImageView
                android:id="@+id/image_battery_right"
                android:padding="@dimen/dp_10"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@mipmap/common_right_1"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toTopOf="@+id/tv_battery_white"/>
            <TextView
                android:textSize="14sp"
                android:textColor="@color/color_FFFFFF"
                android:gravity="center"
                android:id="@+id/tv_status_text_5"
                android:background="@drawable/bg_corners_bg_text_2"
                android:paddingLeft="10dp"
                android:paddingRight="10dp"
                android:layout_width="wrap_content"
                android:layout_height="40dp"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="10dp"
                android:text="@string/qc_text_410"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/tv_battery_white_sub"/>
            <TextView
                android:textSize="20sp"
                android:textColor="@color/app_bg_run_common_1"
                android:id="@+id/tv_auto_start"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="@dimen/dp_10"
                android:maxWidth="300dp"
                android:text="@string/qc_text_403"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/tv_status_text_5"/>
            <ImageView
                android:id="@+id/image_2_right"
                android:padding="@dimen/dp_10"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@mipmap/common_right_1"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toTopOf="@+id/tv_auto_start"/>
            <TextView
                android:textSize="14sp"
                android:textColor="@color/color_FFFFFF"
                android:gravity="center"
                android:id="@+id/tv_status_text_2"
                android:background="@drawable/bg_corners_bg_text_2"
                android:paddingLeft="10dp"
                android:paddingRight="10dp"
                android:layout_width="wrap_content"
                android:layout_height="40dp"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="10dp"
                android:text="@string/qc_text_410"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/tv_auto_start"/>
            <TextView
                android:textSize="20sp"
                android:textColor="@color/app_bg_run_common_1"
                android:id="@+id/tv_auto_delete_title"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="@dimen/dp_10"
                android:maxWidth="300dp"
                android:text="@string/qc_text_404"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/tv_status_text_2"/>
            <TextView
                android:textSize="14sp"
                android:textColor="@color/app_bg_run_common_1"
                android:id="@+id/tv_auto_delete"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="@dimen/dp_10"
                android:maxWidth="300dp"
                android:text="@string/qc_text_405"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/tv_auto_delete_title"/>
            <ImageView
                android:id="@+id/image_3_right"
                android:padding="@dimen/dp_10"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@mipmap/common_right_1"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toTopOf="@+id/tv_auto_delete"/>
            <TextView
                android:textSize="14sp"
                android:textColor="@color/color_FFFFFF"
                android:gravity="center"
                android:id="@+id/tv_status_text_3"
                android:background="@drawable/bg_corners_bg_text_2"
                android:paddingLeft="10dp"
                android:paddingRight="10dp"
                android:layout_width="wrap_content"
                android:layout_height="40dp"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="10dp"
                android:text="@string/qc_text_410"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/tv_auto_delete"/>
            <TextView
                android:textSize="14sp"
                android:textColor="@color/app_bg_run_common_1"
                android:id="@+id/tv_auto_battery"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="@dimen/dp_10"
                android:maxWidth="300dp"
                android:text="@string/qc_text_407"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/tv_auto_battery_title"/>
            <TextView
                android:textSize="20sp"
                android:textColor="@color/app_bg_run_common_1"
                android:id="@+id/tv_auto_battery_title"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="@dimen/dp_10"
                android:maxWidth="300dp"
                android:text="@string/qc_text_406"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/tv_status_text_3"/>
            <ImageView
                android:id="@+id/image_4_right"
                android:padding="@dimen/dp_10"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@mipmap/common_right_1"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toTopOf="@+id/tv_auto_battery"/>
            <TextView
                android:textSize="14sp"
                android:textColor="@color/color_FFFFFF"
                android:gravity="center"
                android:id="@+id/tv_status_text_4"
                android:background="@drawable/bg_corners_bg_text_2"
                android:paddingLeft="10dp"
                android:paddingRight="10dp"
                android:layout_width="wrap_content"
                android:layout_height="40dp"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="10dp"
                android:text="@string/qc_text_410"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/tv_auto_battery"/>
            <TextView
                android:textSize="14sp"
                android:textColor="@color/app_bg_run_common_1"
                android:gravity="center"
                android:padding="@dimen/qc_dp_20"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="60dp"
                android:text="@string/qc_text_408"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/tv_status_text_4"/>
        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.core.widget.NestedScrollView>
</androidx.constraintlayout.widget.ConstraintLayout>
