<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="@dimen/qc_dp_120"
    android:minWidth="@dimen/qc_dp_120">
    <ImageView
        android:id="@+id/image_feedback_image"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:minHeight="@dimen/qc_dp_80"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"/>
    <ImageView
        android:id="@+id/image_delete"
        android:padding="@dimen/qc_dp_8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@mipmap/watch_face_delete"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"/>
</androidx.constraintlayout.widget.ConstraintLayout>
