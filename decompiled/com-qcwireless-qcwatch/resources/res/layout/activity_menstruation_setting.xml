<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <include
        android:id="@+id/title_bar"
        layout="@layout/layout_title_bar"/>
    <com.qcwireless.qcwatch.ui.base.view.QSettingItemWithClickSystem
        android:id="@+id/qc_ms_switch"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/qc_dp_26"
        android:layout_marginRight="@dimen/qc_dp_26"
        android:minHeight="@dimen/qc_dp_54"
        app:LtextColor="@color/menstruation_common_1"
        app:isShowRightText="true"
        app:layout_constraintTop_toBottomOf="@+id/title_bar"
        app:leftText="@string/qc_text_283"
        app:rightStyle="iconSwitch"
        app:rightText=""
        app:rightTextColor="@color/menstruation_common_2"/>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/menstruation_group"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_marginLeft="@dimen/qc_dp_26"
        android:layout_marginRight="@dimen/qc_dp_26"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/qc_ms_switch">
        <View
            android:id="@+id/line_1"
            android:background="@color/menstruation_line_1"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            app:layout_constraintTop_toTopOf="parent"/>
        <TextView
            android:textColor="@color/menstruation_common_2"
            android:id="@+id/tv_title_1"
            android:paddingRight="@dimen/qc_dp_8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/qc_dp_36"
            android:layout_marginTop="@dimen/qc_dp_10"
            android:maxWidth="@dimen/qc_dp_250"
            android:text="@string/qc_text_192"
            app:layout_constraintBottom_toTopOf="@+id/qc_ms_during"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/line_1"/>
        <com.qcwireless.qcwatch.ui.base.view.QSettingItem
            android:id="@+id/qc_ms_during"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:minHeight="@dimen/qc_dp_54"
            app:LtextColor="@color/menstruation_common_1"
            app:isShowRightText="true"
            app:layout_constraintTop_toBottomOf="@+id/tv_title_1"
            app:leftIcon="@mipmap/menstruation_during"
            app:leftText="@string/qc_text_195"
            app:rightStyle="iconShow"
            app:rightTextColor="@color/menstruation_common_2"/>
        <TextView
            android:textColor="@color/menstruation_common_2"
            android:id="@+id/tv_title_2"
            android:paddingRight="@dimen/qc_dp_8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/qc_dp_36"
            android:layout_marginTop="@dimen/qc_dp_10"
            android:maxWidth="@dimen/qc_dp_250"
            android:text="@string/qc_text_193"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/qc_ms_during"/>
        <com.qcwireless.qcwatch.ui.base.view.QSettingItem
            android:id="@+id/qc_ms_cycle"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:minHeight="@dimen/qc_dp_54"
            app:LtextColor="@color/menstruation_common_1"
            app:isShowRightText="true"
            app:layout_constraintTop_toBottomOf="@+id/tv_title_2"
            app:leftIcon="@mipmap/menstruation_cycle"
            app:leftText="@string/qc_text_196"
            app:rightStyle="iconShow"
            app:rightTextColor="@color/menstruation_common_2"/>
        <TextView
            android:textColor="@color/menstruation_common_2"
            android:id="@+id/tv_title_3"
            android:paddingRight="@dimen/qc_dp_8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/qc_dp_36"
            android:layout_marginTop="@dimen/qc_dp_10"
            android:maxWidth="@dimen/qc_dp_250"
            android:text="@string/qc_text_194"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/qc_ms_cycle"/>
        <com.qcwireless.qcwatch.ui.base.view.QSettingItem
            android:id="@+id/qc_ms_last"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:minHeight="@dimen/qc_dp_54"
            app:LtextColor="@color/menstruation_common_1"
            app:isShowRightText="true"
            app:layout_constraintTop_toBottomOf="@+id/tv_title_3"
            app:leftIcon="@mipmap/menstruation_last"
            app:leftText="@string/qc_text_197"
            app:rightStyle="iconShow"
            app:rightTextColor="@color/menstruation_common_2"/>
        <View
            android:id="@+id/line_2"
            android:background="@color/menstruation_line_1"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            app:layout_constraintTop_toBottomOf="@+id/qc_ms_last"/>
        <com.qcwireless.qcwatch.ui.base.view.QSettingItemWithClickSystem
            android:id="@+id/qc_ms_alarm_switch"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:minHeight="@dimen/qc_dp_54"
            app:LtextColor="@color/menstruation_common_1"
            app:isShowRightText="true"
            app:layout_constraintTop_toBottomOf="@+id/line_2"
            app:leftText="@string/qc_text_198"
            app:rightStyle="iconSwitch"
            app:rightText=""
            app:rightTextColor="@color/menstruation_common_2"/>
        <com.qcwireless.qcwatch.ui.base.view.QSettingItem
            android:id="@+id/qc_ms_alarm_1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:minHeight="@dimen/qc_dp_54"
            app:LtextColor="@color/menstruation_common_1"
            app:isShowRightText="true"
            app:layout_constraintTop_toBottomOf="@+id/qc_ms_alarm_switch"
            app:leftText="@string/qc_text_199"
            app:rightStyle="iconShow"
            app:rightText=""
            app:rightTextColor="@color/menstruation_common_2"/>
        <com.qcwireless.qcwatch.ui.base.view.QSettingItem
            android:id="@+id/qc_ms_alarm_2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:minHeight="@dimen/qc_dp_54"
            app:LtextColor="@color/menstruation_common_1"
            app:isShowRightText="true"
            app:layout_constraintTop_toBottomOf="@+id/qc_ms_alarm_1"
            app:leftText="@string/qc_text_200"
            app:rightStyle="iconShow"
            app:rightText=""
            app:rightTextColor="@color/menstruation_common_2"/>
        <com.qcwireless.qcwatch.ui.base.view.QSettingItem
            android:id="@+id/qc_ms_alarm_3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:minHeight="@dimen/qc_dp_54"
            app:LtextColor="@color/menstruation_common_1"
            app:isShowRightText="true"
            app:layout_constraintTop_toBottomOf="@+id/qc_ms_alarm_2"
            app:leftText="@string/qc_text_201"
            app:rightStyle="iconShow"
            app:rightText=""
            app:rightTextColor="@color/menstruation_common_2"/>
        <androidx.constraintlayout.widget.Group
            android:id="@+id/alarm_group"
            android:visibility="gone"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:constraint_referenced_ids="qc_ms_alarm_1,qc_ms_alarm_2,qc_ms_alarm_3"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.constraintlayout.widget.ConstraintLayout>
